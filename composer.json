{
    "name": "jdslv/atoum-report-cobertura",
    "description": "Cobertura report for atoum",
    "version": "1.0.2",
    "type": "library",
    "keywords": [
        "TDD",
        "atoum",
        "test",
        "unit testing",
        "reports",
        "atoum-extension",
        "cobertura",
        "gitlab"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "atoum\\atoum\\reports\\cobertura\\": "src/cobertura"
        },
        "files": [
            "src/cobertura.php",
            "configuration.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "atoum\\atoum\\reports\\cobertura\\tests\\units\\": "tests/units/cobertura"
        }
    },
    "require": {
        "ext-xdebug": "*",
        "ext-xml": "*",
        "atoum/atoum": "^4.0",
        "atoum/reports-extension": "^4.0"
    },
    "require-dev": {
        "atoum/autoloop-extension": "dev-master",
        "jdslv/atoum-xml-extension": "dev-main",
        "satesh/phpcs-gitlab-report": "^1.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "sort-packages": true
    }
}
